    &.effect {
        position: relative;
        overflow: hidden;
        &.fill-vertical:after,
        &.fill-horizontal:after {
            -webkit-transition: all .2s ease-out;
            transition: all .2s ease-out;
        }
        &.icon-bottom {
            span,
            i {
                -webkit-transition: all .2s ease-out;
                transition: all .2s ease-out;
            }
        }
        &.icon-top {
            span,
            i {
                -webkit-transition: all .2s ease-out;
                transition: all .2s ease-out;
            }
        }
        &.icon-right {
            span,
            i {
                -webkit-transition: all .2s ease-out;
                transition: all .2s ease-out;
            }
        }
        &.icon-left {
            span {
                -webkit-transition: all .2s ease-out;
                transition: all .2s ease-out;
            }
            i {
                -webkit-transition: all .2s ease-out;
                transition: all .2s ease-out;
                position: absolute;
                display: block;
                top: 50%;
                left: 0;
                -webkit-transform: translate(-24px, -50%);
                -ms-transform: translate(-24px, -50%);
                transform: translate(-24px, -50%);
                ms-transform: translate(-24px, -50%);
                opacity: 0;
            }
            span {
                width: 100%;
                height: 100%;
                display: block;
                -webkit-transform: translate(0, 0);
                -ms-transform: translate(0, 0);
                transform: translate(0, 0);
                ms-transform: translate(0, 0);
                -webkit-backface-visibility: hidden;
                -ms-backface-visibility: hidden;
                -o-backface-visibility: hidden;
                backface-visibility: hidden;
                ms-backface-visibility: hidden;
            }
        }
    }
    -webkit-transition: all .2s ease-out;
    transition: all .2s ease-out;
    &:hover.effect.icon-left span {
        -webkit-transform: translate(15px, 0);
        -ms-transform: translate(15px, 0);
        transform: translate(15px, 0);
        ms-transform: translate(15px, 0);
    }
    &.small.effect.icon-left {
        i {
            -webkit-transform: translate(-14px, -50%);
            -ms-transform: translate(-14px, -50%);
            transform: translate(-14px, -50%);
        }
        &:hover span {
            -webkit-transform: translate(8px, 0px);
            -ms-transform: translate(8px, 0px);
            transform: translate(8px, 0px);
        }
    }
    &.effect.icon-right {
        i {
            position: absolute;
            display: block;
            top: 50%;
            right: 0;
            -webkit-transform: translate(30px, -50%);
            -ms-transform: translate(30px, -50%);
            transform: translate(30px, -50%);
            ms-transform: translate(30px, -50%);
            opacity: 0;
        }
        span {
            width: 100%;
            height: 100%;
            display: block;
            -webkit-transform: translate(0, 0);
            -ms-transform: translate(0, 0);
            transform: translate(0, 0);
            ms-transform: translate(0, 0);
            -webkit-backface-visibility: hidden;
            -ms-backface-visibility: hidden;
            -o-backface-visibility: hidden;
            backface-visibility: hidden;
            ms-backface-visibility: hidden;
        }
    }
    &:hover.effect.icon-right span {
        -webkit-transform: translate(-15px, 0);
        -ms-transform: translate(-15px, 0);
        transform: translate(-15px, 0);
        ms-transform: translate(-15px, 0);
    }
    &.small.effect.icon-right {
        i {
            -webkit-transform: translate(14px, -50%);
            -ms-transform: translate(14px, -50%);
            transform: translate(14px, -50%);
        }
        &:hover span {
            -webkit-transform: translate(-5px, 0px);
            -ms-transform: translate(-5px, 0px);
            transform: translate(-5px, 0px);
        }
    }
    &.effect.icon-top {
        i {
            position: absolute;
            display: block;
            top: -30px;
            left: 50%;
            -webkit-transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);
            ms-transform: translate(-50%, -50%);
            opacity: 0;
        }
        span {
            width: 100%;
            height: 100%;
            display: block;
            text-align: center;
            -webkit-backface-visibility: hidden;
            -ms-backface-visibility: hidden;
            -o-backface-visibility: hidden;
            backface-visibility: hidden;
            ms-backface-visibility: hidden;
            -webkit-transform: translate(0, 0);
            -ms-transform: translate(0, 0);
            transform: translate(0, 0);
            ms-transform: translate(0, 0);
        }
    }
    &:hover.effect.icon-top span {
        -webkit-transform: translate(0px, 35px);
        -ms-transform: translate(0px, 35px);
        transform: translate(0px, 35px);
        ms-transform: translate(0px, 35px);
    }
    &.small.effect.icon-top i {
        top: -22px;
    }
    &.large.effect.icon-top:hover span {
        -webkit-transform: translate(0px, 54px);
        -ms-transform: translate(0px, 54px);
        transform: translate(0px, 54px);
    }
    &.effect.icon-bottom {
        i {
            position: absolute;
            display: block;
            bottom: -36px;
            left: 50%;
            -webkit-transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);
            ms-transform: translate(-50%, -50%);
            opacity: 0;
        }
        span {
            width: 100%;
            height: 100%;
            display: block;
            text-align: center;
            -webkit-backface-visibility: hidden;
            -ms-backface-visibility: hidden;
            -o-backface-visibility: hidden;
            backface-visibility: hidden;
            ms-backface-visibility: hidden;
            -webkit-transform: translate(0, 0);
            -ms-transform: translate(0, 0);
            transform: translate(0, 0);
            ms-transform: translate(0, 0);
        }
    }
    &:hover.effect.icon-bottom span {
        -webkit-transform: translate(0px, -27px);
        -ms-transform: translate(0px, -27px);
        transform: translate(0px, -27px);
        ms-transform: translate(0px, -27px);
    }
    &.small.effect.icon-bottom i {
        bottom: -27px;
    }
    &.large.effect.icon-bottom:hover span {
        -webkit-transform: translate(0px, -48px);
        -ms-transform: translate(0px, -48px);
        transform: translate(0px, -48px);
    }
    &:hover.effect {
        &.icon-right i,
        &.icon-left i,
        &.icon-top i,
        &.icon-bottom i {
            opacity: 1;
        }
    }
    &.effect {
        &.fill-vertical,
        &.fill-horizontal {
            overflow: hidden;
        }
        &.fill-vertical {
            span {
                position: relative;
                z-index: 1;
            }
            &:after {
                content: "";
                position: absolute;
                width: 100%;
                height: 0;
                top: 50%;
                left: 50%;
                opacity: 0;
                -webkit-transform: translateX(-50%) translateY(-50%) rotate(40deg);
                -ms-transform: translateX(-50%) translateY(-50%) rotate(40deg);
                transform: translateX(-50%) translateY(-50%) rotate(40deg);
            }
            &:hover:after {
                height: 550%;
                opacity: 1;
            }
            &:after {
                background-color: #252525;
            }
            &:hover {
                color: #fff;
            }
        }
        &.fill-horizontal {
            span {
                position: relative;
                z-index: 1;
            }
            &:after {
                content: "";
                position: absolute;
                width: 100%;
                height: 0;
                top: 50%;
                left: 50%;
                opacity: 0;
                -webkit-transform: translateX(-50%) translateY(-11%);
                -ms-transform: translateX(-50%) translateY(-11%);
                transform: translateX(-50%) translateY(-11%);
            }
            &:hover:after {
                height: 550%;
                opacity: 1;
            }
            &:after {
                background-color: #252525;
            }
            &:hover {
                color: #fff;
            }
        }
        &.fill:hover {
            background-color: #252525;
            color: #fff;
        }
    }
    &.aqua.effect {
        &.fill-vertical,
        &.fill-horizontal {
            &:after {
                background-color: #1a9ca7;
            }
            &:hover {
                color: #fff;
            }
        }
        &.fill:hover {
            background-color: #087680;
        }
    }
    &.transparent.effect {
        &.fill-vertical:after,
        &.fill-horizontal:after {
            background-color: #fff;
        }
        &.fill-vertical:hover,
        &.fill-horizontal:hover {
            color: #333;
        }
        &.fill:hover {
            background-color: #fff;
            color: #333;
        }
    }
}

