/* Button: Slide effect*/

.btn.btn-slide {
    border-radius: 50px;
    display: table-cell;
    font-weight: 400;
    opacity: 1;
    overflow: hidden;
    padding: 16px;
    position: relative;
    transition: width 0.4s ease 0s, background 0.4s ease 0s;
    width: 48px;
}
.btn:hover.btn-slide {
    width: 140px;
    background-color: rgb(16, 155, 129);
    border-color: rgb(16, 155, 129);
    text-align: left;
}
.btn.btn-slide > i {
    float: left;
    transition: all 0.8s ease 0s;
}
.btn:hover.btn-slide > i {
    transform: rotate(360deg);
}
.btn.btn-slide > span {
    left: 40px;
    position: absolute;
    text-align: left;
    top: 13px;
    transition: left 0.5s ease 0s, opacity 0.5s ease 0s;
    white-space: nowrap;
    opacity: 0;
}
.btn:hover.btn-slide > span {
    opacity: 1;
}

/* Mini version*/

.btn.btn-xs.btn-slide {
    padding: 6px;
    width: 28px;
    font-size: 11px;

.btn.btn-xs.btn-slide:hover {
    width: 80px;
}
.btn.btn-xs.btn-slide > span {
    top: 0px;
    left: 28px;
}

/* Small version*/

.btn.btn-sm.btn-slide {
    padding: 10px;
    width: 36px;
    font-size: 12px;
}
.btn.btn-sm.btn-slide:hover {
    width: 110px;
}
.btn.btn-sm.btn-slide > span {
    left: 36px;
    top: 0px;
}

/* Large version*/

.btn.btn-lg.btn-slide {
    height: 58px;
    padding: 18px;
    width: 60px;
}
.btn.btn-lg.btn-slide,
.btn.btn-lg.btn-slide > i {
    font-size: 22px;
}
.btn.btn-lg.btn-slide > span {
    top: 2px;
    left: 56px;
}
.btn:hover.btn-lg.btn-slide {
    width: 200px;
}

