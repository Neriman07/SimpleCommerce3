/* Animation buttons */

.animate-btn {
    background: transparent;
    border: 1px solid #19ad90;
    color: #19ad90;
}
.animate-btn:hover {
    background: #19ad90;
    border: 1px solid #19ad90;
    color: #fff;
}
.btn-fall {
    background: transparent;
    border: 1px solid #19ad90;
    color: #19ad90;
}
.btn-fall:after {
    background: #19ad90;
    width: 100%;
    height: 0;
    top: 0;
    left: 0;
    color: #fff;
}
.btn-fall:hover:after {
    color: #fff;
}
.btn-fall:hover:after {
    height: 100%;
}
.btn-progress-left {
    background: transparent;
    border: 1px solid #19ad90;
    color: #19ad90;
}
.btn-progress-left:after {
    background: #19ad90;
    width: 0;
    height: 100%;
    top: 0;
    left: 0;
    color: #fff;
}
.btn-progress-left:hover:after {
    width: 100%;
}
.btn-center {
    background: transparent;
    border: 1px solid #19ad90;
    color: #19ad90;
    overflow: hidden;
}
.btn-center:after {
    height: 80%;
    top: 50%;
    left: 50%;
    background: #19ad90;
    color: #fff;
    opacity: 0;
    border-radius: 1px;
    -webkit-transform: translateX(-50%) translateY(-50%);
    -moz-transform: translateX(-50%) translateY(-50%);
    -ms-transform: translateX(-50%) translateY(-50%);
    transform: translateX(-50%) translateY(-50%);
}
.btn-center:hover:after {
    width: 90%;
    opacity: 1;
}
.btn-vertical {
    background: transparent;
    border: 1px solid #19ad90;
    color: #19ad90;
    overflow: hidden;
}
.btn-vertical:after {
    background: #19ad90;
    color: #fff;
    width: 100%;
    height: 0;
    top: 50%;
    left: 50%;
    opacity: 0;
    -webkit-transform: translateX(-50%) translateY(-50%) rotate(45deg);
    -moz-transform: translateX(-50%) translateY(-50%) rotate(45deg);
    -ms-transform: translateX(-50%) translateY(-50%) rotate(45deg);
    transform: translateX(-50%) translateY(-50%) rotate(45deg);
}
.btn-vertical:hover:after {
    height: 260%;
    opacity: 1;
}
