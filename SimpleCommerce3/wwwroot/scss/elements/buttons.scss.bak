    }
}

/*Button icons*/

.button,
.btn {
    i {
        font-size: 15px;
        margin-right: 10px;
    }
    &.icon-left i {
        margin-right: 10px;
    }
    &.icon-right i {
        margin-left: 10px;
        margin-right: 0;
    }
    &.small {
        font-size: 10px;
        height: 28px;
        line-height: 24px;
        padding: 0 14px;
    }
    &.transparent.small {
        line-height: 0;
    }
    &.small {
        i {
            font-size: 11px;
            margin-right: 6px;
        }
        &.icon-left i {
            margin-right: 6px;
        }
        &.icon-right i {
            margin-left: 6px;
            margin-right: 0;
        }
    }
    &.large {
        font-size: 16px;
        height: 55px;
        letter-spacing: 2px;
        line-height: 52px;
        padding: 0 32px;
    }
    &.transparent.large {
        line-height: 22px;
    }
    &.border:hover {
        opacity: 1;
    }
    &.transparent:hover {
        opacity: 1;
        color: #333 !important;
        span,
        i {
            color: #333 !important;
        }
    }
    &.full-rounded {
        border-radius: 50px;
    }
    &.rounded {
        border-radius: 3px;
    }
    &.fullwidth-button {
        display: block !important;
        margin-left: auto !important;
        margin-right: auto !important;
    }
    &.button-3d {
