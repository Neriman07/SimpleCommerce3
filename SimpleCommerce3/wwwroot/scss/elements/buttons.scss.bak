            &:hover {
                background-color: #fff;
                color: #252525;
                i {
                    color: #252525;
                }
            }
        }
    }
    &.transparent {
        background-color: transparent;
        border-color: #fff;
        border-style: solid;
        border-width: 2px;
        box-shadow: none;
        color: #fff;
        letter-spacing: 2px;
        opacity: 1;
        padding: 16px 36px;
        transition: all 0.2s linear 0s;
        &:hover,
        &:active {
